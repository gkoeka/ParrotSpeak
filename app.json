{
  "expo": {
    "name": "ParrotSpeak",
    "slug": "parrotspeak",
    "owner": "gkoeka",
    "version": "1.0.1",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parrotspeak.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "ParrotSpeak needs access to your microphone to translate your speech."
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4F46E5"
      },
      "package": "com.parrotspeak.app",
      "versionCode": 1,
      "permissions": [
        "RECORD_AUDIO",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "RECORD_AUDIO",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ],
      "runtimeVersion": "1.0.0"
    },
    "androidNavigationBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content",
      "visible": "leanback"
    },
    "plugins": [
      "expo-av",
      "expo-file-system",
      "./plugins/withIapPlayFlavor.cjs"
    ],
    "experiments": {
      "turboModules": false
    },
    "extra": {
      "eas": {
        "projectId": "432bc810-3ba5-4443-ac0d-25978617b7e5"
      },
      "expo-module-scripts": {
        "android": {
          "jsEngine": "hermes",
          "enableNewArchitecture": false
        }
      }
    },
    "updates": {
      "url": "https://u.expo.dev/432bc810-3ba5-4443-ac0d-25978617b7e5"
    }
  }
}

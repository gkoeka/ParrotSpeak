{
  "cli": {
    "version": ">= 3.18.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "env": {
          "EXPO_USE_NEW_ARCH": "0"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_USE_NEW_ARCH": "0"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_USE_NEW_ARCH": "0",
          "EAS_NO_VCS": "1"
        }
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "production"
      }
    }
  }
}
